set number
set relativenumber
syntax on
set showmode
set autoindent
set mouse=a
filetype indent on
set tabstop=4
set shiftwidth=4
set expandtab
set linebreak
set showmatch
set ignorecase
set smartcase
set history=1000
set undolevels=1000
set backspace=indent,eol,start
set termguicolors
set noshowmode
set cursorline


